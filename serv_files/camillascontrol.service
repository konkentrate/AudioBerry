[Unit]
Description=Connection between CamillaDSP and the USB input volume and mute controls
After=multi-user.target camillagui.service
Requires=camillagui.service

[Service]
User=top
Type=idle
ExecStart=python3 /home/top/AudioBerry/cdsp/cam_control/source_control.py

[Install]
WantedBy=multi-user.target